{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {"singleQuote": true, "trailingComma": "es5"}
        ]
    }
}